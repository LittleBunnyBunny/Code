class Solution {
public:
    bool canJump(vector<int>& nums) {
        vector<int> memo;
        for (int i = 0; i < nums.size() - 1; ++i) {
            memo.push_back(2);
        }
        memo.push_back(1);
        for (int i = nums.size() - 2; i >= 0; --i) {
            int maxjump = min(static_cast<int>(nums.size() - 1), i + nums[i]);
            for (int j = i + 1; j <= maxjump; ++j) {
                if (memo[j] == 1) {
                    memo[i] = 1;
                } 
            }
        }
        return (memo[0] == 1);
    }
};




